<html>
    <head>
        <title>Mosileno</title>
        <link rel="stylesheet"
              href="${request.static_url('mosileno:static/mosileno.css')}"
              type="text/css"
              media="screen"
              charset="utf-8"
          />
          <tal:block repeat="reqt css_links|[]">
            <link rel="stylesheet"
                  href="${request.static_url('deform:static/%s' % reqt)}"
                  type="text/css" />
          </tal:block>
          <tal:block repeat="reqt js_links|[]">
            <script type="text/javascript"
                    src="${request.static_url('deform:static/%s' % reqt)}"
             ></script>
          </tal:block>
    </head>
    <body>
        <div class=headerCont>
            <ul class=headerLeft>
                <li><a href="/feed/add">Add a feed</a></li>
                <li><a href="/feed/add/opml">Import OPML file</a></li>
                <li><a href=#>Tutu</a></li>
            </ul>
            <ul class=headerRight>
                <span tal:condition="not logged_in">
                    <li><a href="/signup">Sign up</a></li>
                    <li><a href="/login">Login</a></li>
                </span>
                <span tal:condition="logged_in">
                    <li><a href="/logout">Logout</a></li>
                </span>
                <li>
            </ul>
        </div>
        <div id=main>
            <tal:block metal:define-slot="content">
            </tal:block>
        </div>
        <div class=footer>
            <ul>
                <li><a href=#>About us</a></li>
            </ul>
        </div>
    </body>
</html>
